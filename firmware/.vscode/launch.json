      {
         "name": "debug STM32F030K6 blinky",
         "cwd": "${workspaceRoot}",
         "executable": "${workspaceFolder}/bin/STM32F030K6_blinky/debug/STM32F030K6_blinky.elf",
         "request": "launch",
         "type": "cortex-debug",
         "servertype": "bmp",
         "interface": "swd",
         "device": "STM32F030",
         "BMPGDBSerialPort": "/dev/ttyACM0",
         "runToEntryPoint": "main",
         "preLaunchCommands": [],
         "preRestartCommands": [],
         "preLaunchTask": "build debug STM32F030K6 blinky",
         "svdFile": "${workspaceFolder}/libraries/libMcu/svd/st/STM32F030.svd",
         "debuggerArgs": [
            "--command=./${workspaceFolder}/gdb_scripts/STM32F030K6_blinky_debug.txt"
         ],
      },
